    	<option value="3">Open report</option>
    	<option value="4">Send email</option>
    	<option value="5">Validation</option>
    </selection>
    
    <selection name="studio.action.builder.line.type.select">
    	<option value="0">Formula</option>
    	<option value="1">Groovy</option>
    </selection>
    
    <selection name="studio.view.item.type.select">
    	<option value="0">Field</option>
    	<option value="1">Button</option>
    	<option value="2">Label</option>
    	<option value="3">Spacer</option>
    	<option value="4">Dashlet</option>
    </selection>
    
    <selection name="studio.action.builder.line.validation.type.select">
    	<option value="error">Error</option>
    	<option value="alert">Alert</option>
    	<option value="info">Information</option>
    	<option value="notify">Notification</option>
    </selection>
    
    <selection name="record.import.rule.select">
    	<option value="0">Create</option>
    	<option value="1">Search</option>
    	<option value="2">Search or Create</option>
    </selection>
    
